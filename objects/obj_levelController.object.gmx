            <string>/// tile creation
// initial tile creation
guy = false;
guyRng = irandom(100);
for (var X = 0; X &lt; gridSize; X++) {
    for (var Y = 0; Y &lt; gridSize; Y++) {
        if (gridType[# X, Y] == 1) {
            // land
            var rng = irandom(1000);
            var left = 0 * global.tileSize;
            var top = 0 * global.tileSize;
            if (rng &gt;= 975 &amp;&amp; rng &lt; 990) {
                // 1.5% chance for rocks and grass
                var left = choose(2, 3, 4) * global.tileSize;
                var top = 0 * global.tileSize;
            } else if (rng &gt;= 990 &amp;&amp; rng &lt; 995) {
                // 0.5% chance for sea anemone
                var left = choose(3, 4) * global.tileSize;
                var top = choose(1, 2) * global.tileSize;
            } else if (rng &gt;= 995 &amp;&amp; rng &lt; 998) {
                // 0.3% chance for a seastar
                var left = 2 * global.tileSize;
                var top = 1 * global.tileSize;
            } else if (rng &gt;= 998 &amp;&amp; rng &lt; 1000) {
                // 0.2% chance for a sand castle
                if (X &gt; 0 &amp;&amp; Y &gt; 0 &amp;&amp; gridType[# X-1, Y-1] == 1 &amp;&amp; gridType[# X-1, Y] == 1 &amp;&amp; gridType[# X, Y-1] == 1) {
                    var left = 1 * global.tileSize;
                    var top = 2 * global.tileSize;
                    tile_delete(gridTile[# X-1, Y-1]);
                    gridTile[# X-1, Y-1] = tile_add(tileset, 0, 1 * global.tileSize, global.tileSize, global.tileSize,
                        (X - 1) * global.tileSize, (Y - 1) * global.tileSize, tileDepth);
                    tile_delete(gridTile[# X-1, Y]);
                    gridTile[# X-1, Y] = tile_add(tileset, 0, 2 * global.tileSize, global.tileSize, global.tileSize,
                        (X - 1) * global.tileSize, (Y - 0) * global.tileSize, tileDepth);
                    tile_delete(gridTile[# X, Y-1]);
                    gridTile[# X, Y-1] = tile_add(tileset, 1 * global.tileSize, 1 * global.tileSize, global.tileSize, global.tileSize,
                        (X - 0) * global.tileSize, (Y - 1) * global.tileSize, tileDepth);
                }
            } else if (rng == 1000 &amp;&amp; guyRng &gt;= 95 &amp;&amp; !guy) {
                var loc = gridSize/2
                if ((X &lt; loc-10) || (X &gt; loc+10) || (Y &lt; loc-10) || (Y &gt; loc+10)) {
                    var left = 2 * global.tileSize;
                    var top = 2 * global.tileSize;
                    guy = true;
                }
            }
            var roomX = X * global.tileSize;
            var roomY = Y * global.tileSize;
            
            gridTile[# X, Y] = tile_add(tileset, left, top, global.tileSize, global.tileSize, roomX, roomY, tileDepth);
        } else {
            // goo lagoon
            var left = 1 * global.tileSize;
            var top = 0 * global.tileSize;
            var roomX = X * global.tileSize;
            var roomY = Y * global.tileSize;
            
            gridTile[# X, Y] = tile_add(tileset, left, top, global.tileSize, global.tileSize, roomX, roomY, tileDepth);
            instance_create(roomX, roomY, obj_wall);
        }
    }
}
